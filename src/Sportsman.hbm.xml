<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jshooting.shootingDatabase.Sportsman" table="Sportsmans">
        <id name="id" column="sportsmanId" type="long">
            <generator class="native"></generator>
        </id>
        <property name="name" column="sportsmanName" type="string"></property>
        <many-to-one name="team" class="com.jshooting.shootingDatabase.Team" column="sportsmanTeam"/>
   
    </class>
</hibernate-mapping>