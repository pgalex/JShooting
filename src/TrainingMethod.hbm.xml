<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class name="com.jshooting.model.TrainingMethod" table="TrainingMethods">
    <id column="trainingMethodId" name="id" type="long">
      <generator class="native"/>
    </id>
    <property column="trainingMethodName" name="name" type="string"/>
  </class>
</hibernate-mapping>
