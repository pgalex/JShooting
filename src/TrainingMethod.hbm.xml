<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jshooting.shootingDatabase.TrainingMethod" table="TrainingMethods">
        <id name="id" column="trainingMethodId" type="long">
            <generator class="native"></generator>
        </id>
        <property name="name" column="trainingMethodName" type="string"></property>
    </class>
</hibernate-mapping>