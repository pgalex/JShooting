<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" 
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.jshooting.shootingDatabase.Place" table="Places">
        <id name="id" column="placeId" type="long">
            <generator class="native"></generator>
        </id>
        <property name="name" column="placeName" type="string"></property>
        <property column="placeBeginDate" name="beginDate" not-null="true" type="date"/>
        <property column="placeEndDate" name="endDate" not-null="true" type="date"/>
    </class>
</hibernate-mapping>
